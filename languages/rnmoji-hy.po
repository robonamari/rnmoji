msgstr "Էմոջիներ - %s ազատ տեղ"

msgid "Search Emojis"
msgstr "Որոնել էմոջիներ"

msgid "List of uploaded emojis with actions"
msgstr "Վերբեռնված էմոջիների ցուցակ գործողություններով"

msgid "Emoji"
msgstr "Էմոջի"

msgid "Emoji Name"
msgstr "Էմոջիի անուն"

msgid "Actions"
msgstr "Գործողություններ"

msgid "At least 2 chars, alphanumeric and underscores only"
msgstr "Առնվազն 2 նիշ, միայն ալֆանման թվեր և ստորակետեր"

msgid "Rename emoji name"
msgstr "Վերանվանել էմոջիի անունը"

